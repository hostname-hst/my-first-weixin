<template name="starsTemplate">
  <view class='stars-container'>
    <view class='star'>
    <block wx:for="{{stars}}" wx:for-item="item">
      <image wx:if="{{item==1}}" src='/images/icon/star.png'></image>
       <image wx:else src='/images/icon/none-star.png'></image> 
    </block>
      
    </view>
    <text>{{sc}}</text>
  </view>
</template>